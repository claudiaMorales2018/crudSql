<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycrudapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyCrudApp">
        <activity android:name=".MainActivityDetalle"></activity>
        <activity android:name=".MainActivityEliminar" />
        <activity android:name=".MainActivityActualizar" />
        <activity android:name=".MainActivityBuscar" />
        <activity android:name=".MainActivity_Mostrar" />
        <activity android:name=".MainActivity_insertar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>